<VBox styleClass="background"  alignment="CENTER" prefHeight="300" prefWidth="515" spacing="20" xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="controller.RecordController">
    <Label text="Season Record" styleClass="label-h2"/>
    <TableView fx:id="recordsTableView" prefWidth="500">
        <columns>
            <TableColumn text="Round" fx:id="roundTableColumn" prefWidth="125"/>
            <TableColumn text="Game" fx:id="gamesTableColumn" prefWidth="125"/>
            <TableColumn text="Winning Team" fx:id="winningTeamTableColumn" prefWidth="125"/>
            <TableColumn text="Losing Team" fx:id="losingTeamTableColumn" prefWidth="125"/>
        </columns>
    </TableView>
    <Button text="close" onAction="#close"/>
    <HBox>
    </HBox>
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>
